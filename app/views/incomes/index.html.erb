<%- model_class = Income -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:tenant_id) %></th>
      <th><%= model_class.human_attribute_name(:condominio_id) %></th>
      <th><%= model_class.human_attribute_name(:income_tipe_id) %></th>
      <th><%= model_class.human_attribute_name(:apartment_id) %></th>
      <th><%= model_class.human_attribute_name(:forma_pagamento_id) %></th>
      <th><%= model_class.human_attribute_name(:date) %></th>
      <th><%= model_class.human_attribute_name(:payment) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @incomes.each do |income| %>
      <tr>
        <td><%= income.id %></td>
        <td><%= income.tenant_id %></td>
        <td><%= income.condominio_id %></td>
        <td><%= income.income_tipe_id %></td>
        <td><%= income.apartment_id %></td>
        <td><%= income.forma_pagamento_id %></td>
        <td><%= income.date %></td>
        <td><%= income.payment %></td>
        <td><%=l income.created_at %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      tenant_income_path(income, tenant_id: @tenant.id), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_tenant_income_path(income, tenant_id: @tenant.id), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      tenant_income_path(income, tenant_id: @tenant.id),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_tenant_income_path(tenant_id: @tenant.id),
            :class => 'btn btn-primary' %>
