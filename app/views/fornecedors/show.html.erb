<%- model_class = Fornecedor -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nome) %>:</strong></dt>
  <dd><%= @fornecedor.nome %></dd>
  <dt><strong><%= model_class.human_attribute_name(:pf) %>:</strong></dt>
  <dd><%= @fornecedor.pf %></dd>
  <dt><strong><%= model_class.human_attribute_name(:documento) %>:</strong></dt>
  <dd><%= @fornecedor.documento %></dd>
  <dt><strong><%= model_class.human_attribute_name(:contato) %>:</strong></dt>
  <dd><%= @fornecedor.contato %></dd>
  <dt><strong><%= model_class.human_attribute_name(:telefone) %>:</strong></dt>
  <dd><%= @fornecedor.telefone %></dd>
  <dt><strong><%= model_class.human_attribute_name(:email) %>:</strong></dt>
  <dd><%= @fornecedor.email %></dd>
  <dt><strong><%= model_class.human_attribute_name(:site) %>:</strong></dt>
  <dd><%= @fornecedor.site %></dd>
  <dt><strong><%= model_class.human_attribute_name(:endereco) %>:</strong></dt>
  <dd><%= @fornecedor.endereco %></dd>
  <dt><strong><%= model_class.human_attribute_name(:observacao) %>:</strong></dt>
  <dd><%= @fornecedor.observacao %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tenant_id) %>:</strong></dt>
  <dd><%= @fornecedor.tenant_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tipo_fornecedor_id) %>:</strong></dt>
  <dd><%= @fornecedor.tipo_fornecedor_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              tenant_fornecedors_path(tenant_id: @tenant.id), :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_tenant_fornecedor_path(@fornecedor, tenant_id: @tenant.id), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              tenant_fornecedor_path(@fornecedor, tenant_id: @tenant.id),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
