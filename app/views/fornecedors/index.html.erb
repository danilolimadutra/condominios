<%- model_class = Fornecedor -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:nome) %></th>
      <th><%= model_class.human_attribute_name(:pf) %></th>
      <th><%= model_class.human_attribute_name(:documento) %></th>
      <th><%= model_class.human_attribute_name(:contato) %></th>
      <th><%= model_class.human_attribute_name(:telefone) %></th>
      <th><%= model_class.human_attribute_name(:email) %></th>
      <th><%= model_class.human_attribute_name(:site) %></th>
      <th><%= model_class.human_attribute_name(:endereco) %></th>
      <th><%= model_class.human_attribute_name(:observacao) %></th>
      <th><%= model_class.human_attribute_name(:tenant_id) %></th>
      <th><%= model_class.human_attribute_name(:tipo_fornecedor_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @fornecedors.each do |fornecedor| %>
      <tr>
        <td><%= link_to fornecedor.id, tenant_fornecedor_path(fornecedor, tenant_id: @tenant.id) %></td>
        <td><%= fornecedor.nome %></td>
        <td><%= fornecedor.pf %></td>
        <td><%= fornecedor.documento %></td>
        <td><%= fornecedor.contato %></td>
        <td><%= fornecedor.telefone %></td>
        <td><%= fornecedor.email %></td>
        <td><%= fornecedor.site %></td>
        <td><%= fornecedor.endereco %></td>
        <td><%= fornecedor.observacao %></td>
        <td><%= fornecedor.tenant_id %></td>
        <td><%= fornecedor.tipo_fornecedor_id %></td>
        <td><%=l fornecedor.created_at %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      tenant_fornecedor_path(fornecedor, tenant_id: @tenant.id), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_tenant_fornecedor_path(fornecedor, tenant_id: @tenant.id), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      tenant_fornecedor_path(fornecedor, tenant_id: @tenant.id),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_tenant_fornecedor_path(tenant_id: @tenant.id),
            :class => 'btn btn-primary' %>
